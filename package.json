{
  "name": "neurallempire",
  "version": "1.0.0",
  "description": "AI-powered lead generation and business automation platform",
  "main": "index.html",
  "scripts": {
    "dev": "npx serve . -p 8000",
    "build": "echo 'Building static assets...' && echo 'Build complete!'",
    "deploy:netlify": "./deploy.sh netlify",
    "deploy:vercel": "./deploy.sh vercel",
    "deploy:github": "./deploy.sh github",
    "deploy:surge": "./deploy.sh surge",
    "deploy:s3": "./deploy.sh s3",
    "test": "echo 'Running tests...' && npm run test:html && npm run test:js",
    "test:html": "echo 'Validating HTML...' || true",
    "test:js": "echo 'Testing JavaScript...' || true",
    "lint": "echo 'Linting code...' || true",
    "preview": "npx serve . -p 3000",
