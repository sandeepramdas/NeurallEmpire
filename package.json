  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sandeepramdaz/NeurallEmpire.git"
  },
  "homepage": "https://neurallempire.com",
  "devDependencies": {
    "serve": "^14.2.0"
  },
  "optionalDependencies": {
    "netlify-cli": "^17.0.0",
    "vercel": "^33.0.0",
    "surge": "^0.23.0",
    "@aws-cli/client-s3": "^3.0.0",
    "html5validator": "^0.4.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
